      "source": [
        "num =32456\n",
        "def mainloop(num: int) -> int:\n",
        "  numstr = str(num)\n",
        "  nummap = map(int, numstr)\n",
        "  list1 = list(nummap)\n",
        "  sum = 0\n",
        "  leng = len(list1)\n",
        "  i = 0\n",
        "  while i < leng:\n",
        "    sum = sum + list1[i]\n",
        "    i += 1\n",
        "  return sum\n",
        "\n",
        "while num >=10:\n",
        "   num = mainloop(num)\n",
        "print(num)\n"
